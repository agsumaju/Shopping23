@model Shopping23.Data.Entities.Country

@{
    ViewData["Title"] = "Delete";
}

<h1>Borrar</h1>

<h3>Esta seguro de desear borrar?</h3>
<div>

    <partial name="_CountryDetails" />
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Borra" class="btn btn-outline-danger" /> 
        <a asp-action="Index" class="btn btn-outline-success">Regresar</a>
    </form>
</div>
